{
    "manifest_version": 2,
    "name": "Time2Eat", 
    "version": "1.0",
    "permissions": ["gcm", "storage", "notifications", "background"],
    "icons": {
        "128": "./icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "peristent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Get notified when lunch has arrived at the office",
        "default_popup": "popup.html"
    }
}
